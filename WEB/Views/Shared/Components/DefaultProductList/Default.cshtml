@model IEnumerable<WEB.DTOs.ProductDtos.ResultProductDto>
<section class="locations-1" id="locations">
        <div class="locations py-5">
            <div class="container py-lg-5 py-md-4 py-2">
                <div class="heading text-center mx-auto">
                    <h3 class="title-big">Topp Eiendommer</h3>
                </div>
                <div class="row pt-md-5 pt-4">
                    @foreach(var item in Model)
                    {
                    <div class="col-lg-4 col-md-6 mb-3">
                        <a href="/Default/Index">
                            <div  class="box16">
                                <div class="rentext-listing-category"><span>@item.Type</span><span>@item.Name</span></div>
                                <img class="img-fluid" style="height: 200px; width: 600px;" src="@item.ImageUrl" alt="">
                                <div class="box-content">
                                    <h3 class="title">NOK @item.Price.ToString("N0")</h3>
                                    <span class="post">@item.Street, @item.City, @item.PostalCode @item.Country</span>
                                </div>
                            </div>
                        </a>
                    </div>
                    }
                </div>
            </div>
        </div>
    </section>


