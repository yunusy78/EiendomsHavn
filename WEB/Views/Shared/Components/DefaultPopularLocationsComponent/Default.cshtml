@model List<WEB.DTOs.CitiyDtos.ResultCityDto>

<section class="locations-1 popular" id="locations">
    <div class="locations py-5">
        <div class="container py-lg-5 py-md-4">
            <div class="heading text-center mx-auto">
                <h6 class="title-small">Utforsk byer</h6>
                <h3 class="title-big">Populære steder</h3>
            </div>
            <div class="row pt-5">
                @foreach(var item in Model)
                {
                <div class="col-lg-3 col-md-4 col-6 mb-3">
                    <a href="#url">
                        <div class="box16">
                            <img class="img-fluid" style="width: 400px; height: 200px;" src="@item.ImageUrl" alt="">
                            <div class="box-content">
                                <h3 class="title mb-1">@item.City</h3>
                                <span class="post">@item.CityCount Annonse</span>
                            </div>
                        </div>
                    </a>
                </div>
}
            </div>
            </div>
        </div>
    </section>

