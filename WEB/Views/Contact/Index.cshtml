<!-- contacts -->
<section class="w3l-contact-7 pt-5" id="contact">
    <div class="contacts-9 pt-lg-5 pt-md-4">
        <div class="container">
            <div class="top-map">
                <div class="row map-content-9">
                    <div class="col-lg-8">
                        <div class="contact-form">
                            <h5 class="mb-2">Ta kontakt</h5>
                            <p class="mb-4">E-postadressen din vil ikke bli publisert. Påkrevde felt er merket med *</p>
                            <form action="/Contact/Create" method="post" class="">
                                <div class="form-grid">
                                    <div class="input-field">
                                        <input type="text" name="ContactName" id="w3lName" placeholder="Ditt Navn"
                                               required="">
                                    </div>
                                    <div class="input-field">
                                        <input type="email" name="Email" id="w3lSender" placeholder="E-Post"
                                               required="">
                                    </div>
                                </div>
                                <div class="input-field mt-4">
                                    <textarea name="Message" id="w3lMessage" placeholder="Melding"></textarea>
                                </div>
                                 <input type="checkbox"> <label>Lagre mitt navn, e-post og nettside i denne nettleseren for neste gang jeg kommenterer</label>
                                 <button type="submit" class="btn btn-primary btn-stil mt-3">Send</button>
                            </form>
                        </div>
                    </div>
                    <div class="col-lg-4 cont-details">
                        <adresse>
                            <h5 class="">Vår kontoradresse</h5>
                            <p><span class="fa fa-map-marker"></span>Estate salgskontor, 32, Min gate, Kingston, Arendal 48023. </p>
                        
                            <h5 class="mt-4 pt-lg-3">Telefoninformasjon</h5>
                            <p><span class="fa fa-mobile"></span> <strong>Telefon :</strong>
                                <a href="tel:+1(12) 366 411 4999"> (+47) 366 411 499</a></p>
                        
                            <p><span class="fa fa-phone"></span> <strong>Tlf :</strong>
                                <a href="tel:+1(12) 366 411 4999"> (+47) 366 411 499</a></p>
                        
                            <p> <span class="fa fa-envelope"></span> <strong>E-post :</strong>
                                <a href="mailto:mail@example.com"> Eindomshavn@example.com</a></p>
                        </adresse>
                    </div>
                </div>
            </div>
        </div>
    <div class="contacts-9 pt-lg-5 pt-md-4">
        <div class="container">
            <div class="top-map">
                <div class="row map-content-9">
                    <div class="col-lg-8">
                        <div class="contact-form">
                            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d7604.055561004179!2d8.76563977526058!3d58.46113741582449!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x46415d15ef3db8dd%3A0xf7a15a2546b29d9a!2sArendal%2C%20Norway!5e0!3m2!1sen!2sno!4v1630309906822!5m2!1sen!2sno" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
</section>
